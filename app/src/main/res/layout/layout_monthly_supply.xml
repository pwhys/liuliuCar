<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View style="@style/a_layout_style_hline" />

    <com.cheweibao.liuliu.ui.BaseGridView
        android:id="@+id/gv_monthly_supply"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:horizontalSpacing="1dip"
        android:listSelector="@color/white_gray"
        android:numColumns="3"
        android:scrollbars="none"
        android:stretchMode="columnWidth"
        android:verticalSpacing="0dip" />

    <View style="@style/a_layout_style_hline" />

    <View
        android:id="@+id/view_monthly_supply"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/transparent_30" />


</LinearLayout>